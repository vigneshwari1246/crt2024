import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
        int a,b,c;
        float d;
        a=s.nextInt();
        b=s.nextInt();
        c=s.nextInt();
        d=s.nextFloat();
        int r1=a*a;
        System.out.println(r1);
        int r2=b*c;
        System.out.println(r2);
        float r3=(float)3.14*d*d;
        System.out.printf("%.2f",r3);
        
    }
}
