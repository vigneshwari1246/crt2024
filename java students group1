import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
            int a;
    a=s.nextInt();
    if(a>=100 && a<=200)
    {
        int r=a/4;
        System.out.printf("%d\n%d\n%d\n%d",r,r,r,r+a%4);
    }
    else
    {
        System.out.println("INVALID INPUT");   
    }
}
}
