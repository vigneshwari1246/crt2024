import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {  
      Scanner s=new Scanner(System.in);  
        int a,b;
    a=s.nextInt();
    b=s.nextInt();
    int r=a*b;
    if(r<=1000)
      System.out.printf("%d",r);
     else
       System.out.printf("%d",a+b);
    }
}
