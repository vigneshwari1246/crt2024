import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner s=new Scanner(System.in);
    char ch;
        ch=s.next().charAt(0);
    if(ch>='A'&& ch<='Z')
        System.out.println("Upper");
    else if(ch>='a'&& ch<='z')
       System.out.println("Lower");
    else if(ch>='0'&& ch<='9')
        System.out.println("Number");
    else
        System.out.println("Symbol");
    }
}
